<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">

    <!--<configuration>-->
    <!--<date-format>yyyy-MM-dd HH:mm:ss</date-format>-->
    <!--</configuration>-->
    <configuration>
        <custom-converters>
            <converter
                    type="ir.mine.project.base.dozer.dozerConvertor.DozerDateCustomConverterGeneric">
                <class-a>java.time.ZonedDateTime
                </class-a>
                <class-b>java.lang.String
                </class-b>
            </converter>
            <converter
                    type="ir.mine.project.base.dozer.dozerConvertor.DozerDateCustomConverterInteger">
                <class-a>java.time.ZonedDateTime
                </class-a>
                <class-b>java.lang.Integer
                </class-b>
            </converter>
            <converter
           
                    type="ir.mine.project.base.dozer.dozerConvertor.DozerEnumCustomConverter">
                <class-a>java.lang.Enum
                </class-a>
                <class-b>java.lang.String
                </class-b>
            </converter>

        </custom-converters>

    </configuration>

</mappings>